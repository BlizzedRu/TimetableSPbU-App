<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="launcherCredentialsColor" format="color" />

    <declare-styleable name="LoadableContentLayout">
        <attr name="loadingView" format="reference"/>
        <attr name="errorView" format="reference"/>
        <attr name="emptyViewId" format="reference"/>
        <attr name="status" format="enum">
            <enum name="loading" value="0"/>
            <enum name="error" value="1"/>
            <enum name="content" value="2"/>
            <enum name="empty" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="SearchView">
        <attr name="iconsColor" format="color"/>
        <attr name="searchBackground" format="reference"/>
        <attr name="searchTextColor" format="color"/>
        <attr name="searchHintColor" format="color"/>
        <attr name="disabled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CircleTitleView">
        <attr name="circleColor" format="color"/>
    </declare-styleable>


</resources>
